<player-frame-list>
        <frame frame-id="player-ready-west" img-location="Resources\Sprites\Player\ReadyWest\">
                <frame-layer-list>
                        <frame-layer layer-name="race-layer" z-order="1">
                                <layer-img object-key="race-tonosian" img="Tonosian_Ready_West_Base.png"/>
                        </frame-layer>
                        <frame-layer layer-name="leg-layer" z-order="2">
                                <layer-img object-key="basic-leggings" img="Ready_West_Basic_Leggings.png"/>
                        </frame-layer>
                        <frame-layer layer-name="feet-layer" z-order="3">
                                <layer-img object-key="basic-boots" img="Ready_West_Basic_Boots.png"/>
                        </frame-layer>
                        <frame-layer layer-name="torso-layer" z-order="4">
                                <layer-img object-key="basic-torso" img="Ready_West_Basic_Torso.png"/>
                        </frame-layer>
                        <frame-layer layer-name="item-layer" z-order="5">
                                <layer-img  object-key="basic-sword" img="Ready_West_Basic_Sword.png"/>
                                <layer-img  object-key="royal-sword" img="Ready_West_Royal_Sword.png"/>
                        </frame-layer>
                        <frame-layer layer-name="hands-layer" z-order="6">
                                <layer-img object-key="basic-gloves" img="Ready_West_Basic_Gloves.png"/>
                        </frame-layer>
                </frame-layer-list>
